{
	"schemaVersion": 1,
	"id": "minecraft-screenshot-web-endpoint",
	"version": "${version}",
	"name": "Minecraft Screenshot Web Endpoint",
	"description": "A mod that adds a web endpoint to get screenshots from the game",
	"authors": [
		"NickAcPT"
	],
	"contact": {
		"homepage": "https://github.com/NickAcPT/minecraft-screenshot-web-endpoint",
		"sources": "https://github.com/NickAcPT/minecraft-screenshot-web-endpoint"
	},
	"license": "MIT",
	"icon": "assets/minecraft-screenshot-web-endpoint/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			{
				"value": "io.github.nickacpt.minecraftscreenshotendpoint.MinecraftScreenshotEndpointMod",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"minecraft-screenshot-web-endpoint.mixins.json"
	],
	"depends": {
		"minecraft": "~1.19.4",
		"java": ">=17",
		"fabric-language-kotlin": ">=1.8.22"
	}
}